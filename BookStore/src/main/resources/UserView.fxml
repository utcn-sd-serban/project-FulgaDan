<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="450.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ro.utcn.fulgadan.BookStore.presentation.controller.UserViewController">
   <children>
      <Button fx:id="shoppingCart" defaultButton="true" layoutX="394.0" layoutY="26.0" mnemonicParsing="false" onAction="#handleCartButton" text="Shopping Cart">
         <font>
            <Font name="Berlin Sans FB" size="13.0" />
         </font>
      </Button>
      <Button fx:id="myAccount" defaultButton="true" layoutX="510.0" layoutY="26.0" mnemonicParsing="false" onAction="#handleAccountButton" text="My account">
         <font>
            <Font name="Berlin Sans FB" size="13.0" />
         </font>
      </Button>
      <Button fx:id="logOut" defaultButton="true" layoutX="609.0" layoutY="26.0" mnemonicParsing="false" onAction="#handleLogOutButton" text="Log out">
         <font>
            <Font name="Berlin Sans FB" size="13.0" />
         </font>
      </Button>
      <ListView fx:id="listView" layoutX="80.0" layoutY="58.0" onMouseClicked="#openBook" prefHeight="371.0" prefWidth="540.0" />
   </children>
</AnchorPane>
